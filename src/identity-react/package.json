    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "auth0-js": "^9.16.0",
    "date-fns": "^2.21.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "http-server": "^0.12.3",
    "jwks-rsa": "^2.0.3",
    "material-table": "^1.69.3",
    "material-ui-phone-number": "^2.2.6",
    "nan": "^2.14.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.17.7",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
