{
  "name": "web-identity",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.6.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@types/auth0-js": "^9.12.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "auth0-js": "^9.12.2",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "http-server": "^0.12.1",
    "jwks-rsa": "^1.6.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "reactn": "^2.2.5",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:headless": "docker run --rm -it -v $PWD:/e2e -w /e2e --network host cypress/included:3.8.3",
    "test:headless:log": "docker run --rm -it -e ELECTRON_ENABLE_LOGGING=1 -v $PWD:/e2e -w /e2e --network host cypress/included:3.8.3",
    "eject": "react-scripts eject",
    "dev": "npm-run-all --parallel start server",
    "server": "node server.js",
    "deploy-dev": "aws s3 sync ./build/ s3://profile-dev.languagetechnology.org/ --profile identity",
    "deploy-prod": "aws s3 sync ./build/ s3://profile.languagetechnology.org/ --profile identity"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@passport-next/passport-mocked": "^1.3.0",
    "@types/react-router-dom": "^5.1.3",
    "cypress": "^4.2.0",
    "node-mocks-http": "^1.8.1",
    "passport": "^0.4.1",
    "pg": "^7.18.1",
    "sequelize": "^5.21.3",
    "whatwg-fetch": "^3.0.0"
  },
  "proxy": "http://localhost:3001"
}
