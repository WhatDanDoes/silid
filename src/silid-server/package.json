{
  "name": "silid-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "local": "node ./bin/www",
    "deploy": "serverless deploy -v",
    "dev": "nodemon ./bin/www",
    "test": "NODE_ENV=test npx jasmine",
    "docker:build-dev": "docker build --build-arg environment=development_aws -t silid-server .",
    "docker:tag-dev": "docker tag silid-server:latest 117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server-dev:latest",
    "docker:deploy-dev": "docker push 117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server-dev:latest",
    "docker:build-prod": "docker build --build-arg environment=production -t silid-server .",
    "docker:tag-prod": "docker tag silid-server:latest 117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server:latest",
    "docker:deploy-prod": "docker push 117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server:latest",
    "ecr:login": "$(aws ecr get-login --no-include-email --region us-east-1)",
    "ecs:register-task-dev": "aws ecs register-task-definition --cli-input-json file://task-dev.json",
    "ecs:update-service-dev": "aws ecs update-service --cluster default --service arn:aws:ecs:us-east-1:117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server-dev-service --task-definition silid-server-dev-task",
    "ecs:register-task-prod": "aws ecs register-task-definition --cli-input-json file://task-prod.json",
    "ecs:update-service-prod": "aws ecs update-service --cluster default --service arn:aws:ecs:us-east-1:117995318043.dkr.ecr.us-east-1.amazonaws.com/silid-server-service --task-definition silid-server-task"
  },
  "dependencies": {
    "aws-sdk": "^2.636.0",
    "connect-session-sequelize": "^6.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv-flow": "^3.1.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "http-errors": "~1.6.3",
    "http-proxy": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.2",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "serverless-http": "^2.3.0"
  },
  "devDependencies": {
    "@hapi/hapi": "^18.4.1",
    "crypto-js": "^3.1.9-1",
    "hapi-cors": "^1.0.3",
    "hapi-require-https": "^4.0.0",
    "jasmine": "^3.5.0",
    "json-web-key": "^0.4.0",
    "nock": "^11.7.0",
    "node-jose": "^1.1.3",
    "node-jwk": "^0.1.0",
    "node-mocks-http": "^1.8.0",
    "node-rsa": "^1.0.6",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemon": "^1.19.4",
    "pem": "^1.14.3",
    "pem-jwk": "^2.0.0",
    "sequelize-fixtures": "^1.1.1",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "uuidv4": "^6.0.5",
    "zombie": "^6.1.4"
  }
}
